                 xmlns:dc="http://purl.org/dc/elements/1.1/"
                 xmlns:cc="http://creativecommons.org/ns#"
                 xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
                 xmlns:svg="http://www.w3.org/2000/svg"
                 xmlns="http://www.w3.org/2000/svg"
                 version="1.1"
                 viewBox="0 0 4.4979166 6.3500001"
                 height="24"
                 width="17"
                 class="sprinkle-0">
                <g
                   transform="translate(-103.84896,-152.85878)">
                  <path
                     d="m 105.96562,153.12337 v 0.26459 h 0.26459 v -0.26459 z m -0.79375,0.26459 v 0.26458 h 0.26459 v -0.26458 z m 1.32292,0.26458 v 0.26458 h 0.26458 v -0.26458 z m 1.05833,0 v 0.26458 h 0.26459 v -0.26458 z m -1.5875,0.26458 v 0.26459 h 0.26459 v -0.26459 z m -0.79375,0.52917 v 0.26458 h 0.26459 v -0.26458 z m 1.32292,0 v 0.26458 h 0.26458 v -0.26458 z m -1.32292,0.52917 v 0.26458 h 0.26459 v -0.26458 z m 2.64584,0 v 0.26458 h 0.26458 v -0.26458 z m -1.5875,0.26458 v 0.26458 h 0.26458 v -0.26458 z m -2.11667,0.26458 v 0.26459 h 0.26458 v -0.26459 z m 1.05833,0.52917 v 0.26458 h 0.26459 v -0.26458 z m 1.05834,0 v 0.26458 h 0.26458 v -0.26458 z m 1.32291,0 v 0.26458 h 0.26459 v -0.26458 z m -1.85208,0.52917 v 0.26458 h 0.26458 v -0.26458 z m 0.79375,0.79375 v 0.26458 h 0.26458 v -0.26458 z" />
